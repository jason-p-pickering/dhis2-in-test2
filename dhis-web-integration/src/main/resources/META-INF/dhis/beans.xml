<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="
http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

	<!-- DataIntegration Home -->
	<bean id="org.hisp.dhis.integration.action.IntegrationHomeAction"
		class="org.hisp.dhis.integration.action.IntegrationHomeAction"
		scope="prototype">
	  </bean>

	<!-- RIMS -->
	<bean id="rimsService"
		class="org.hisp.dhis.integration.rims.impl.DefaultRIMSService"
		scope="prototype">
    <property name="dataSetService">
			<ref bean="org.hisp.dhis.dataset.DataSetService" />
		</property>
		<property name="dataElementService">
			<ref bean="org.hisp.dhis.dataelement.DataElementService" />
		</property>
    <property name="periodService">
      <ref bean="org.hisp.dhis.period.PeriodService" />
    </property>
	</bean>

	<bean id="org.hisp.dhis.integration.rims.action.RIMSDataElementsListAction"
		class="org.hisp.dhis.integration.rims.action.RIMSDataElementsListAction"
		scope="prototype">
		<property name="rimsService">
			<ref bean="rimsService" />
		</property>
		<property name="expressionService">
			<ref bean="org.hisp.dhis.expression.ExpressionService" />
		</property>		
	</bean>

	<bean id="org.hisp.dhis.integration.rims.action.RIMSOrgunisListAction"
		class="org.hisp.dhis.integration.rims.action.RIMSOrgunisListAction"
		scope="prototype">
		<property name="rimsService">
			<ref bean="rimsService" />
		</property>
		<property name="organisationUnitService">
			<ref bean="org.hisp.dhis.organisationunit.OrganisationUnitService" />
		</property>		
	</bean>
			
	<bean id="org.hisp.dhis.integration.rims.action.RIMSExportFormAction"
		class="org.hisp.dhis.integration.rims.action.RIMSExportFormAction"
		scope="prototype">
		<property name="rimsService">
			<ref bean="rimsService" />
		</property>
	</bean>
	
  <bean id="org.hisp.dhis.integration.rims.action.RIMSImportFormAction"
		class="org.hisp.dhis.integration.rims.action.RIMSImportFormAction"
		scope="prototype">
		<property name="rimsService">
			<ref bean="rimsService" />
		</property>
	</bean>
  
	<bean id="org.hisp.dhis.integration.rims.action.RIMSExportResultAction"
		class="org.hisp.dhis.integration.rims.action.RIMSExportResultAction"
		scope="prototype">
		<property name="rimsService">
			<ref bean="rimsService" />
		</property>
		<property name="statementManager">
			<ref bean="org.hisp.dhis.aggregation.batch.statement.StatementManager" />
		</property>
		<property name="dataSetService">
			<ref bean="org.hisp.dhis.dataset.DataSetService" />
		</property>
		<property name="periodService">
			<ref bean="org.hisp.dhis.period.PeriodService" />
		</property>
		<property name="organisationUnitService">
			<ref bean="org.hisp.dhis.organisationunit.OrganisationUnitService" />
		</property>
		<property name="dataElementService">
			<ref bean="org.hisp.dhis.dataelement.DataElementService" />
		</property>
		<property name="aggregationService">
			<ref bean="org.hisp.dhis.aggregation.AggregationService" />
		</property>
		<property name="dataValueService">
			<ref bean="org.hisp.dhis.datavalue.DataValueService" />
		</property>
		<property name="dataElementCategoryOptionComboService">
			<ref bean="org.hisp.dhis.dataelement.DataElementCategoryOptionComboService" />
		</property>
		
	</bean>
	
		
	<bean id="org.hisp.dhis.integration.rims.action.GetRIMSDataElementsAction"
		class="org.hisp.dhis.integration.rims.action.GetRIMSDataElementsAction"
		scope="prototype">
		<property name="rimsService">
			<ref bean="rimsService" />
		</property>
	</bean>

	<bean id="org.hisp.dhis.integration.rims.action.GetRIMSPHCsAction"
		class="org.hisp.dhis.integration.rims.action.GetRIMSPHCsAction"
		scope="prototype">
		<property name="rimsService">
			<ref bean="rimsService" />
		</property>
	</bean>
  
  <bean id="org.hisp.dhis.integration.rims.action.RIMSImportResultAction"
		class="org.hisp.dhis.integration.rims.action.RIMSImportResultAction"
		scope="prototype">
		<property name="rimsService">
			<ref bean="rimsService" />
		</property>
		<property name="statementManager">
			<ref bean="org.hisp.dhis.aggregation.batch.statement.StatementManager" />
		</property>
		<property name="dataSetService">
			<ref bean="org.hisp.dhis.dataset.DataSetService" />
		</property>
		<property name="periodService">
			<ref bean="org.hisp.dhis.period.PeriodService" />
		</property>
		<property name="organisationUnitService">
			<ref bean="org.hisp.dhis.organisationunit.OrganisationUnitService" />
		</property>
		<property name="dataElementService">
			<ref bean="org.hisp.dhis.dataelement.DataElementService" />
		</property>
		<property name="dataValueService">
			<ref bean="org.hisp.dhis.datavalue.DataValueService" />
		</property>
		<property name="dataElementCategoryOptionComboService">
			<ref bean="org.hisp.dhis.dataelement.DataElementCategoryOptionComboService" />
		</property>
    </bean>
  
  <bean class="org.hisp.dhis.integration.rims.action.TruncateAction">
  </bean>
  
  <bean class="org.hisp.dhis.integration.rims.action.TruncateConfirmAction">
  </bean>
